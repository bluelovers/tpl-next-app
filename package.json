  "main": "next.config.js",
  "scripts": {
    "coverage": "npx nyc yarn run test",
    "lint": "npx eslint **/*.ts",
    "ncu": "npx yarn-tool ncu -u",
    "build": "yarn run next:build",
    "dev": "yarn run next:dev",
    "export": "yarn run next:export",
    "start": "yarn run next:start",
    "build:clear": "yarn run rimraf .next && yarn run rimraf out",
    "next:build": "yarn run build:clear && next build",
    "next:dev": "next dev",
    "next:export": "yarn run next:build && next export",
    "next:start": "next start",
    "now:build": "yarn run build:clear",
    "now:deploy": "yarn run now:build && now deploy",
    "npm:publish": "npm publish",
    "postpublish_": "git commit -m \"publish new version\" .",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "sort-package-json": "npx sort-package-json ./package.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc:default": "tsc -p tsconfig.json",
    "tsc:esm": "tsc -p tsconfig.esm.json"
  },
